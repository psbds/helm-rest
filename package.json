    "@stryker-mutator/mocha-runner": "^2.4.0",
    "@stryker-mutator/typescript": "^2.4.0",
    "@types/chai": "^4.2.5",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.12",
    "@types/q": "^1.5.2",
    "@types/request-promise": "^4.1.45",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "mocha-junit-reporter": "^1.23.1",
    "npm-watch": "^0.6.0",
    "nyc": "^14.1.1",
    "sinon": "^7.5.0",
