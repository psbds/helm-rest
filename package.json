  },
  "homepage": "https://github.com/psbds/helm-rest#readme",
  "dependencies": {
    "body-parser": "1.18.2",
    "compression": "1.7.1",
    "cookie-parser": "1.4.3",
    "cors": "2.8.5",
    "helmet": "3.9.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "tsyringe": "^4.0.1"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^2.4.0",
